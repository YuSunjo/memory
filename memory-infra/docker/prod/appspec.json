{
  "version": "1",
  "Resources": [
    {
      "TargetService": {
        "Type": "AWS::ECS::Service",
        "Properties": {
          "TaskDefinition": "<TASK_DEFINITION>",
          "LoadBalancerInfo": {
            "ContainerName": "my-memory",
            "ContainerPort": 8080
          },
          "PlatformVersion": "1.4.0",
          "CapacityProviderStrategy": [
            {
              "CapacityProvider": "FARGATE",
              "Base": 0,
              "Weight": 1
            }
          ]
        }
      }
    }
  ],
  "Hooks": []
}